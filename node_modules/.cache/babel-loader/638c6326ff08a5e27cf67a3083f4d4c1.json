器 */\r\n.index .artilce {\r\n  margin: 10px;\r\n  padding: 5px;\r\n  padding-bottom: 10px;\r\n  border-bottom: 1px solid #999;\r\n}\r\n.index .artilce:last-child {\r\n  border: none;\r\n  margin-bottom: 0;\r\n}\r\n.mx{\r\n  border-radius: 10px;\r\n}\r\n/* 文章标题 */\r\n.index .artilce .artilce-title {\r\n  font-size: 16px;\r\n  font-weight: bolder;\r\n  color: #1a1a1a;\r\n  line-height: 22px;\r\n}\r\n/* 文章容器 */\r\n.index .artilce .artilce-wrapper {\r\n  display: flex;\r\n  align-items: center; /*项目在交叉轴上的对齐方式 */\r\n  margin-top: 5px;\r\n}\r\n/* 图片 */\r\n.index .artilce .artilce-wrapper .wrapper-img img {\r\n  width: 80px;\r\n  height: 80px;\r\n  border-radius: 5px;\r\n  margin-right: 15px;\r\n}\r\n/* 文章内容 */\r\n.index .artilce .artilce-wrapper .wrapper-text {\r\n  font-size: 15px;\r\n  font-weight: 400;\r\n  line-height: 21px;\r\n  color: #444;\r\n  height: 64px;\r\n  overflow: hidden;\r\n}\r\n.index .foont {\r\n  margin-bottom: 72px;\r\n}\r\n.index .shang{\r\n  position: fixed;\r\n  right: 2px;\r\n  bottom: 85px;\r\n}\r\n</style>\r\n<script>\r\n// 引入vuex的辅助函数\r\nimport {mapState} from 'vuex'\r\nimport {mapGetters} from 'vuex'\r\nimport {mapMutations} from 'vuex'\r\nimport {mapActions} from 'vuex'\r\n export default {\r\n  data() {\r\n    return {\r\n      url:null,\r\n      banler:[]\r\n    }\r\n  },\r\n  methods:{\r\n    //获取vuex中的修改返回地址函数\r\n    // ...mapMutations(['fanhuiurl']),\r\n    chanp(){//产品查看更多\r\n    localStorage.setItem('fanhuiISurl',location.pathname )\r\n      this.$router.push('/chanp');\r\n      //把当前页的地址存储到vuex\r\n      // this.fanhuiurl('/index')\r\n      // this.url=location.pathname \r\n      console.log(this.url)\r\n      \r\n    },\r\n    kepu(){//科普查看更多\r\n    localStorage.setItem('fanhuiISurl',location.pathname )\r\n      this.$router.push('/kepu')\r\n      // this.fanhuiurl('/index')\r\n      \r\n    },\r\n    sousuo(){//搜索\r\n      // this.fanhuiurl('/index')\r\n      localStorage.setItem('fanhuiISurl',location.pathname )\r\n    },\r\n    style(){//摄影分类\r\n       localStorage.setItem('fanhuiISurl',location.pathname )\r\n    },\r\n    camerama(){\r\n       localStorage.setItem('fanhuiISurl',location.pathname )\r\n      this.$router.push('/camerama')\r\n    }\r\n  },\r\n  mounted(){\r\n      this.axios.get('/banler').then(res=>{\r\n        var obj=res.data\r\n        // console.log(obj)\r\n        for(var value of obj){\r\n          console.log(value.two_tu)\r\n          value.two_tu=require(`../assets/banler/${value.two_tu}`)\r\n          this.banler.push(value)\r\n          console.log(value.two_tu)\r\n        }\r\n      //  console.log(this.banler)\r\n        \r\n\r\n      })\r\n    \r\n  }\r\n}\r\n</script>"],"sourceRoot":"src/views"}]}