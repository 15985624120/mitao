tonClass: \"cancelButton\",\r\n        confirmButtonClass: \"confirmButton\",\r\n        confirmButtonText: \"确定\",\r\n        cancelButtonText: \"暂不\",\r\n      })\r\n        .then((action) => {\r\n          if (action == \"confirm\") {\r\n            //点击确定的回调\r\n            // 删除用户名\r\n            localStorage.removeItem('uname')\r\n            // 删除登录状态\r\n            localStorage.removeItem('uname-show')\r\n            this.$toast({\r\n                message: \"已退出登录\",\r\n                position: \"top\",\r\n                duration: \"1500\",\r\n            });\r\n            this.$router.push('/index')\r\n          }\r\n        })\r\n        .catch((err) => {\r\n          if (err == \"cancel\") {\r\n            //点击暂不的回调\r\n          }\r\n        });\r\n    },\r\n  },\r\n};\r\n</script>"]}]}